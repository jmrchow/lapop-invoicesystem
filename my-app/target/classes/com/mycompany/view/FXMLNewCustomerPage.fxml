<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane fx:controller="com.mycompany.view.NewCustomerUI" fx:id="gridpane0"
    xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
<padding><Insets top="25" right="25" bottom="10" left="25"/></padding>

<Label text="Company Name" 
        GridPane.columnIndex="0" GridPane.rowIndex="0"
        GridPane.columnSpan="1"/>
 
    <Label text="Contact Name"
        GridPane.columnIndex="1" GridPane.rowIndex="0"
        GridPane.columnSpan="1"/>
        
    <Label text="Address 1"
        GridPane.columnIndex="2" GridPane.rowIndex="0"
        GridPane.columnSpan="1"/>
        
    <Label text="Address 2"
        GridPane.columnIndex="3" GridPane.rowIndex="0"
        GridPane.columnSpan="1"/>
    
    <Label text="City"
    	GridPane.columnIndex="4" GridPane.rowIndex="0"
    	GridPane.columnSpan="1"/>
    
    <Label text ="Province"
    	GridPane.columnIndex="5" GridPane.rowIndex="0"
    	GridPane.columnSpan="1"/>
    	
    <Label text ="Postal Code"
    	GridPane.columnIndex="6" GridPane.rowIndex="0"
    	GridPane.columnSpan="1"/>
    	
    	
    <Button text="Submit" fx:id="btn" onAction="#handleSubmitButtonAction"
    	GridPane.columnIndex="6" GridPane.rowIndex="3"
    	GridPane.columnSpan="1"/>
    	    
</GridPane>